{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"lib": ["es2018", "DOM", "DOM.Iterable"],
		"outDir": "web",
		"sourceMap": false,
		"strict": true,
		"rootDir": "typescript",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
